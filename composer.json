{
  "name": "k5n/webcalendar",
  "description": "Multi-user web-based calendar app",
  "homepage": "https://k5n.us/webcalendar/",
  "license": "GPL-2.0-only",
  "type": "project",
  "version": "1.9.13",
  "authors": [
    {
      "name": "Craig Knudsen",
      "email": "craig@k5n.us",
      "homepage": "https://k5n.us",
      "role": "Project Maintainer / Developer"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "source"
