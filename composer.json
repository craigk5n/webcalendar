    "mcp/sdk": "^0.3",
    "phpmailer/phpmailer": "6.8.*",
    "tinymce/tinymce": "7.7.*",
    "twbs/bootstrap": "4.6.*",
    "twbs/bootstrap-icons": "1.10.*"
  },
  "require-dev": {
    "components/normalize.css": "^3.0",
    "friendsofphp/php-cs-fixer": "^3.86",
    "phpunit/phpunit": "9.6.*",
    "squizlabs/php_codesniffer": "^3.13"
  }
}
